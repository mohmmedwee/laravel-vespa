{
  "name": "ostah/laravel-vespa",
  "description": "A Laravel package for integrating with Vespa",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Mohammed Alostah",
      "email": "eloustah@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "guzzlehttp/guzzle": "^7.0",
    "predis/predis": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "YourVendor\\Vespa\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "YourVendor\\Vespa\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "YourVendor\\Vespa\\VespaServiceProvider"
      ],
      "aliases": {
        "Vespa": "YourVendor\\Vespa\\Facades\\Vespa"
      }
    }
  }
}
